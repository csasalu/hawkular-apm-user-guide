{ 
  "gitbook": "3.2.2",
  "structure": { 
    "summary": "menu.adoc" 
  },
  "root": "docs",
  "title": "Hawkular-APM",
  "plugins": ["fontsettings", "github", "edit-link", "scripts", "-search", "-sharing", "comment"],
  "pluginsConfig": {
    "fontSettings": {
      "theme": "white",
      "size": 3
    },
    "github": {
      "url": "https://github.com/hawkular/hawkular-apm"
    },
    "theme-default": {
      "showLevel": false
    },
    "edit-link": {
      "base": "https://github.com/hawkular/hawkular-apm-user-guide/edit/master/docs",
      "label": "Edit This Page"
    },
    "scripts": {
      "files": [
        "scripts/utils.js"
      ]
    },
    "comment": {
      "highlightCommented": true
    }
  }
}
